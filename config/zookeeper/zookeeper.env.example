# Zookeeper connectivity placeholders
ZK_ENABLED=false
ZK_CONNECT_STRING=zk1.example.com:2181,zk2.example.com:2181,zk3.example.com:2181/peviitor
ZK_CLIENT_CHROOT=/peviitor
ZK_SECURE=false
ZK_SSL_CA_PATH=/path/to/ca.pem
ZK_SSL_CERT_PATH=/path/to/client.pem
ZK_SSL_KEY_PATH=/path/to/client.key
ZK_TIMEOUT_MS=10000
