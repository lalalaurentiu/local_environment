            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="get the documents only in an array without version" enabled="true">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonOutput

// Get full JSON response from the previous sampler
def response = prev.getResponseDataAsString()


int start = vars.get(&quot;start&quot;).toInteger()
start += 1000
vars.put(&quot;start&quot;, start.toString())


// Parse JSON
def json = new JsonSlurper().parseText(response)

// Extract docs array
def docsArray = json.response.docs

// Remove _version_ field from each doc in the array
docsArray.each { doc -&gt;
    doc.remove(&apos;_version_&apos;)
}

// Convert the cleaned docs array back to JSON string (just the array)
def cleanedDocsJson = JsonOutput.prettyPrint(JsonOutput.toJson(docsArray))

// Store the cleaned docs array JSON string in a JMeter variable
vars.put(&quot;response_cleaned&quot;, cleanedDocsJson)


if (docsArray.size() == 0 ) {
    vars.put(&quot;loop&quot;, &quot;stop&quot;)
} else {
    vars.put(&quot;loop&quot;, &quot;loop&quot;)
}

</stringProp>
