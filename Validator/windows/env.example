# This is an example file. Copy this to .env and fill in your actual values.
# Do NOT commit the .env file to version control.

# --- Repository URLs ---
# These are the repositories that the setup script will clone.
FRONTEND_REPO=https://github.com/peviitor-ro/validator-ui.git
BACKEND_REPO=https://github.com/peviitor-ro/validator-backend.git

# --- PostgreSQL Database ---
# These credentials are used by Docker Compose to set up the database container.
POSTGRES_DB=validator
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# --- Django Superuser ---
# These credentials will be used by the create_superuser.py script.
DJANGO_SUPERUSER_EMAIL=admin@example.com
DJANGO_SUPERUSER_PASSWORD=your_super_secret_password
